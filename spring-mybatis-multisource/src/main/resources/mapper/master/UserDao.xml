<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hy.mybatis.dao.master.UserDao">


    <insert id="save" parameterType="com.hy.mybatis.domain.User">
        insert into user(name,birth_date,salary,status,last_login_time)
        values(#{name},#{birthDate},#{salary},#{status},#{lastLoginTime})
    </insert>


</mapper>